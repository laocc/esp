{
  "name": "core/2.1",
  "title": "esp core:composer dump-autoload --optimize",
  "require": {
    "php": ">7.0"
  },
  "require-dev": {
    "php": ">7.0"
  },
  "autoload": {
    "files": [
      "helper/Functions.php",
      "helper/Bootstrap.php"
