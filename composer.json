{
  "name": "laocc/esp",
  "description": "Efficient Simple PHP",
  "require": {
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "ext-xml": "*",
    "ext-gd": "*",
    "ext-exif": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-mongodb": "*",
    "ext-memcached": "*",
    "ext-memcache": "*",
    "php": ">7.0"
  },
  "require-dev": {
    "php": ">7.0"
  },
  "autoload": {
    "files": [
      "helper/Functions.php",
      "helper/Bootstrap.php"
    ],
    "psr-4": {
      "esp\\core\\": "core/",
      "esp\\plugs\\": "plugs/",
      "esp\\library\\": "library/",
      "esp\\library\\ext\\": "library/ext/",
      "model\\": "application/models/"
    }
  }
}
